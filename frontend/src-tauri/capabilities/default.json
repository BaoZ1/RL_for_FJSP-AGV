{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "http:default",
    "shell:allow-open",
    "shell:allow-spawn",
    "dialog:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://localhost:8000"
        }
      ]
    },
    "websocket:default"
  ]
}